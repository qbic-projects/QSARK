process{
   withName:'.*'{
      time = 144.h
   }
   withName:'FASTP'{
    cpus = 16
   }
   withName:'GATK4_APPLYBQSR'{
       memory = 16.GB
   }
   withName:'BWAMEM1_MEM' {
      memory = 60.GB
   }
  withName:'DEEPVARIANT'{
         errorStrategy = 'ignore'
     scratch = false
     ext.args = { params.wes ?  "--model_type WES --intermediate_results_dir . " : "--model_type WGS --intermediate_results_dir . " }
  }
  withName:'NFCORE_SAREK:SAREK:BAM_VARIANT_CALLING_GERMLINE_ALL:BAM_VARIANT_CALLING_DEEPVARIANT:MERGE_DEEPVARIANT_VCF'{
      errorStrategy = 'ignore'
  }
  withName:'NFCORE_SAREK:SAREK:BAM_VARIANT_CALLING_GERMLINE_ALL:BAM_VARIANT_CALLING_DEEPVARIANT:MERGE_DEEPVARIANT_GVCF'{
        errorStrategy = 'ignore'
  }
  withName:'LEARNREADORIENTATIONMODEL|BCFTOOLS_SORT'{
   errorStrategy = 'ignore'
  }
  withName:'FREEBAYES'{
     memory = 18.GB
  }

}
